{
  "name": "ton-starter-twa-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "precommit": "./pipeline.sh",
    "prepare": "husky install",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "configbot": "node configure.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPatterns=\"__tests__/.*\\.test\\.(ts|tsx)$\" --testPathIgnorePatterns=\"integration\"",
    "test:integration": "jest --testPathPatterns=integration",
    "test:bdd": "cucumber-js",
    "test:bdd:ci": "cucumber-js --profile ci",
    "test:bdd:debug": "cucumber-js --profile debug",
    "test:bdd:html": "cucumber-js --format html:reports/cucumber-report.html",
    "test:payments": "jest --testPathPatterns=payments",
    "test:components": "jest --testPathPatterns=components",
    "test:services": "jest --testPathPatterns=services",
    "test:hooks": "jest --testPathPatterns=hooks",
    "test:all": "npm run test:setup && npm run test:unit && npm run test:integration && npm run test:bdd",
    "test:setup": "node scripts/test-setup.js",
    "test:clean": "rimraf reports coverage test-data",
    "lint": "eslint .",
    "test:cypress": "cypress run",
    "test:e2e": "start-server-and-test dev http://localhost:5173 'yarn cypress run'",
    "reports": "npm run test:setup && mkdir -p reports && npm run test:coverage && npm run test:bdd:html",
    "test:ci": "npm run test:setup && npm run test:unit && npm run test:bdd:ci"
  },
  "dependencies": {
    "@account-abstraction/contracts": "^0.6.0",
    "@alchemy/aa-alchemy": "^3.19.0",
    "@alchemy/aa-core": "^0.1.0",
    "@auth0/auth0-react": "^2.3.0",
    "@babel/core": "^7.0.0",
    "@babel/traverse": "7.25.7",
    "@orbs-network/ton-access": "^2.2.2",
    "@privy-io/react-auth": "^1.99.1",
    "@tanstack/react-query": "^5.59.15",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/react": "^16.1.0",
    "@tonconnect/ui-react": "^2.0.0-beta.2",
    "@twa-dev/sdk": "^7.10.1",
    "@types/crypto-js": "^4.2.2",
    "@types/mercadopago": "^1.5.11",
    "@types/qrcode": "^1.5.5",
    "axios": "^1.11.0",
    "browserify-sign": "^4.2.3",
    "buffer": "^6.0.3",
    "crypto-js": "^4.2.0",
    "ethers": "^5.7.2",
    "glob": "^10.2.7",
    "is-mobile": "^5.0.0",
    "jest-cli": "^29.7.0",
    "lucide-react": "^0.461.0",
    "mercadopago": "^2.8.0",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-ga4": "^2.1.0",
    "react-helmet-async": "^2.0.5",
    "react-is": "^18.3.1",
    "react-router-dom": "^7.3.0",
    "styled-components": "^6.1.13",
    "three": "^0.176.0",
    "ton": "^13.9.0",
    "ton-core": "^0.53.0",
    "ton-crypto": "^3.2.0",
    "use-local-storage": "^3.0.0",
    "userop": "^0.3.4",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vite-plugin-pwa": "^1.0.3"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^11.3.0",
    "@cucumber/pretty-formatter": "^2.1.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.8.0",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/cucumber": "^6.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.3",
    "@types/qrcode.react": "^1.0.5",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "cucumber-html-reporter": "^7.2.0",
    "cucumber-tsflow": "^4.5.2",
    "cypress": "^13.15.0",
    "eslint": "^9.0.0",
    "eslint-formatter-table": "^7.32.1",
    "eslint-plugin-react": "^7.35.0",
    "globals": "^15.9.0",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.8",
    "msw": "^2.11.0",
    "postcss": "^8.4.49",
    "start-server-and-test": "bahmutov/start-server-and-test",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.10",
    "vitest": "^3.1.3",
    "wait-on": "^8.0.1"
  }
}
