{
  "name": "ton-starter-twa-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "precommit": "./pipeline.sh",
    "prepare": "husky install",
    "dev": "vite",
    "build": "tsc && vite build",
    "build:clean": "./scripts/build.sh",
    "preview": "vite preview",
    "configbot": "node configure.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPatterns=\"__tests__/.*\\.test\\.(ts|tsx)$\" --testPathIgnorePatterns=\"integration\"",
    "test:integration": "jest --testPathPatterns=integration",
    "test:bdd": "echo 'BDD tests temporarily disabled' && exit 0",
    "test:bdd:ci": "echo 'BDD CI tests temporarily disabled' && exit 0",
    "test:bdd:debug": "cucumber-js --config cucumber.cjs --profile debug",
    "test:bdd:html": "cucumber-js --config cucumber.cjs --format html:reports/cucumber-report.html",
    "test:payments": "echo 'Payment tests temporarily disabled' && exit 0",
    "test:components": "jest --testPathPatterns=components",
    "test:services": "jest --testPathPatterns=services",
    "test:hooks": "jest --testPathPatterns=hooks",
    "test:all": "npm run test:setup && npm run test:unit && npm run test:integration && npm run test:bdd",
    "test:setup": "node scripts/test-setup.js",
    "test:clean": "rimraf reports coverage test-data",
    "lint": "eslint .",
    "test:cypress": "cypress run",
    "test:e2e": "start-server-and-test dev http://localhost:5173 'yarn cypress run'",
    "reports": "npm run test:setup && mkdir -p reports && npm run test:coverage && npm run test:bdd:html",
    "test:ci": "npm run test:setup && npm run test:unit && npm run test:bdd:ci",
    "security:check": "node scripts/security-check.js",
    "security:audit": "npm audit --audit-level=high",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
